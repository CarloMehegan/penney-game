   "metadata": {},
   "outputs": [],
   "source": [
    "for deck in tqdm(simulation):\n",
    "    bin_deck = bin(deck)\n",
    "    if len(bin_deck) != 54:\n",
    "        bin_deck = '0b' + ('0' * (54 - len(bin_deck))) + bin_deck[2:]\n"
   ]
